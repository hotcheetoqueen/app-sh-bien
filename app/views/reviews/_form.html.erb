<%= simple_form_for @review, data: { turbo: false } do |f| %>
    <%= f.input :title %>
    <%= f.input :restaurant %>
    <%= f.input :body %>
    <%= f.input :score %>
    <%= f.input :price %>
    <%= f.input :cuisine %>
    <%= f.input :ambiance %>
    <%= f.input :address %>
    <%= f.input :phone_number %>

    <%= f.button :submit %>
<% end %>